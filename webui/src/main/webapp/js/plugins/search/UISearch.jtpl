<div style="padding: 10px">
  <div>
    <%var query = ""; %>
    <%if(xdocQuery != null) query = xdocQuery.query; %>
    <div style="padding: 5px 0px">
      <input name="search" value="<%=query%>" placeholder="Search Query" style="width: 600px; height: 20px; border: 1px solid lightgray" />
      <span class="ui-action onSearch">Search</span>
    </div>
  </div>

  <div id="UISearchResult" style="padding-top: 15px"></div>
</div>
